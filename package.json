{
  "name": "hela-config-tunnckocore",
  "version": "0.0.0-semantic-release",
  "description": "A [hela][] shareable config  (preset of tasks) for @tunnckoCore GitHub organization",
  "repository": "tunnckoCore/hela-config-tunnckocore",
  "author": "Charlike Mike Reagent (https://charlike.online)",
  "nspId": "d89cae02-66b5-4a0a-911e-811b665f4af2",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "scripts": {
    "start": "hela",
    "test": "yarn start test",
    "precommit": "yarn start precommit",
    "commit": "yarn start commit",
    "prepublishOnly": "yarn start protect"
  },
  "license": "tunnckoCore-1.0",
  "licenseStart": 2017,
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "files": [
    "src/",
    "rollup.config.js"
  ],
  "dependencies": {
    "@std/esm": "0.12.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-istanbul": "4.1.5",
    "builtin-modules": "1.1.1",
    "eslint": "4.9.0",
    "eslint-config-standard-tunnckocore": "1.0.10",
    "eslint-config-tunnckocore": "0.2.0",
    "husky": "0.14.3",
    "is-ci": "1.0.10",
    "isobject": "3.0.1",
    "nyc": "11.2.1",
    "prettier": "1.7.4",
    "prettier-eslint": "8.2.1",
    "prettier-eslint-cli": "4.4.0",
    "rimraf": "2.6.2",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2",
    "semantic-release": "8.2.0",
    "simple-commit-message": "3.3.2"
  },
  "devDependencies": {
    "hela": "1.3.11",
    "mukla": "0.4.9"
  },
  "keywords": [
    "automation"
  ],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "dush",
        "p-map-series",
        "execa",
        "hela",
        "mri"
      ],
      "highlight": "hela"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": []
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
